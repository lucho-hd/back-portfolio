{
    "rewrites": [
        { 
            "source": "/api(.*)", 
            "destination": "/api/index.js" 
        }
    ],
    "headers": [
        {
            "key": "Access-Control-Allow-Credentials",
            "value": "true"
        },
        {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
        },
        {
            "key": "Access-Control-Allow-Meathods",
            "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" 
        },
        {
            "key": "Access-Control-Allow-Headers",
            "value:" "auth-token, X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
        },
    ]
}
